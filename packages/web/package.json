{
  "name": "journaly",
  "version": "1.0.0",
  "description": "A foreign-language journaling application.",
  "repository": {
    "type": "git",
    "url": "git://github.com/Journaly/journaly.git"
  },
  "author": "Journaly",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Journaly/journaly/issues"
  },
  "main": "index.js",
  "scripts": {
    "build:nexus": "cross-env NEXUS_REFLECTION=1 ts-node -r tsconfig-paths/register -P tsconfig-base.json -T nexus/index.ts",
    "build:next": "next build",
    "build": "npm run prisma:generate && npm run build:nexus && npm run build:next",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev": "node server/index.js",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,md}'",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "svgr": "node scripts/svgr.js",
    "type-check": "tsc",
    "seed": "ts-node -r tsconfig-paths/register -P tsconfig-base.json scripts/seed.ts",
    "db:snapshot": "pg_dump $DATABASE_URL > snapshot.sql",
    "db:restore": "npm run empty-db && cat snapshot.sql | psql $DATABASE_URL",
    "prisma:generate": "npm explore '@journaly/j-db-client' -- npm run prisma:generate",
    "migrate:up": "npm explore '@journaly/j-db-client' -- npm run migrate:up",
    "migrate:save": "npm explore '@journaly/j-db-client' -- npm run migrate:save",
    "empty-db": "psql $DATABASE_URL  -c 'DROP SCHEMA public CASCADE; CREATE SCHEMA public;'",
    "reseed-db": "npm run empty-db && npm run migrate:up && npm run seed",
    "translations:topics:pull": "ts-node -r tsconfig-paths/register -P tsconfig-base.json scripts/pull-topic-translations.mjs",
    "translations:topics:push": "ts-node -r tsconfig-paths/register -P tsconfig-base.json scripts/push-topic-translations.mjs"
  },
  "prisma": {
    "schema": "node_modules/@journaly/j-db-client/prisma/schema.prisma"
  },
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "@journaly/j-db-client": "1.0.4",
    "@nexus/schema": "^0.19.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/diff": "^4.0.2",
    "@types/escape-html": "^1.0.0",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/nodemailer": "^6.4.0",
    "@udecode/slate-plugins": "^0.72.5",
    "apollo-server-micro": "^2.19.0",
    "aws-sdk": "^2.737.0",
    "bcryptjs": "^2.4.3",
    "chroma-js": "^2.1.1",
    "classnames": "^2.2.6",
    "cookie": "^0.4.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.11.1",
    "date-fns-tz": "^1.1.3",
    "diff": "^4.0.2",
    "dotenv": "^8.2.0",
    "escape-html": "^1.0.3",
    "graphql": "^15.4.0",
    "is-hotkey": "^0.1.6",
    "is-url": "^1.2.4",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "next": "^10.0.5",
    "next-i18next": "^8.2.0",
    "nexus-plugin-prisma": "^0.25.0",
    "nprogress": "^0.2.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-hook-form": "^5.7.2",
    "react-toastify": "^6.0.8",
    "reactjs-popup": "^2.0.4",
    "slate": "https://journaly-hosted-packages.s3.us-east-2.amazonaws.com/slate-v0.60.3-journaly.0.tgz",
    "slate-history": "https://journaly-hosted-packages.s3.us-east-2.amazonaws.com/slate-history-v0.60.3-journaly.0.tgz",
    "slate-hyperscript": "https://journaly-hosted-packages.s3.us-east-2.amazonaws.com/slate-hyperscript-v0.60.3-journaly.0.tgz",
    "slate-react": "https://journaly-hosted-packages.s3.us-east-2.amazonaws.com/slate-react-v0.60.3-journaly.0.tgz",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "uuid": "^8.3.2",
    "xss": "^1.0.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.1",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-react-apollo": "1.13.1",
    "@next/bundle-analyzer": "^10.0.5",
    "@prisma/cli": "^2.11.0",
    "@svgr/core": "^5.4.0",
    "@svgr/plugin-jsx": "^5.4.0",
    "@svgr/plugin-prettier": "^5.4.0",
    "@svgr/plugin-svgo": "^5.4.0",
    "@types/chroma-js": "^2.1.3",
    "@types/classnames": "^2.2.10",
    "@types/cookie": "^0.4.0",
    "@types/express": "^4.17.11",
    "@types/is-url": "^1.2.28",
    "@types/node": "^14.14.31",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "colors": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "express": "^4.17.1",
    "husky": "^4.2.3",
    "p-retry": "^4.2.0",
    "prettier": "^2.0.1",
    "pretty-quick": "^2.0.1",
    "prompts": "^2.3.2",
    "typescript": "^3.8.3"
  }
}
